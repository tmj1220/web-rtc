{
  "name": "rokid-rtc-sdk",
  "version": "4.4.0",
  "description": "Rokid Web RTC SDK",
  "module": "./src/index.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "build-SDK": "npm run clean && rollup --c rollup.config.ts",
    "build-device": "npm run clean && rollup --c build/device/rollup.config.ts",
    "devBuild": "cross-env NODE_ENV=dev webpack --config webpack.config.js",
    "testBuild": "cross-env NODE_ENV=test webpack --config webpack.config.js",
    "prodBuild": "cross-env NODE_ENV=prod webpack --config webpack.config.js",
    "test": "jest --coverage --verbose -u",
    "load": "webpack",
    "start": "webpack serve --mode=development --config webpack.config.js",
    "deploy": "cd lib && npm publish --registry https://npm.rokid-inc.com/repository/npm-releases/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-external-helpers": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^20.9.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "5.11.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.8.0",
    "eslint-plugin-jest": "^26.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "mock-socket": "^9.1.2",
    "prettier": "^2.5.1",
    "rollup": "^2.67.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "antd": "^4.18.8",
    "axios": "^0.26.0",
    "core-js": "^3.22.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2"
  }
}
